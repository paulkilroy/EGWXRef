{
	"name": "EGW XRef",
	"version": "1.0",
	"manifest_version": 2,
	"icons": { "128": "egw.png" },
	"content_scripts": [
		{
			"matches": ["*://*.biblegateway.com/*"],
			"js": ["xref.js", "content.js"],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": ["egw.png", "modal.html", "modal.js"]
}
